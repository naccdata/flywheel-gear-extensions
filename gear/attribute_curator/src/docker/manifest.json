{
    "name": "attribute-curator",
    "label": "Attribute Curator",
    "description": "Curation of derived data for MQT project",
    "version": "0.0.1",
    "author": "NACC",
    "maintainer": "NACC <nacchelp@uw.edu>",
    "cite": "",
    "license": "MPL-2.0",
    "url": "https://naccdata.github.io/flywheel-gear-extensions",
    "source": "https://github.com/naccdata/flywheel-gear-extensions",
    "environment": {
        "FLYWHEEL": "/flywheel/v0"
    },
    "custom": {
        "gear-builder": {
            "category": "analysis",
            "image": "naccdata/attribute-curator:0.0.1"
        },
        "flywheel": {
            "suite": "Curation",
            "show-job": true
        }
    },
    "inputs": {
        "api-key": {
            "base": "api-key",
            "read-only": false
        }
    },
    "config": {
        "dry_run": {
            "description": "Whether to do a dry run",
            "type": "boolean",
            "default": false
        },
        "derive_rules": {
            "description": "CSV file containing the derivation rules",
            "type": "string"
        },
        "date_key": {
            "description": "Date key to order data by",
            "type": "string"
        },
        "acquisition_labels": {
            "description": "Comma-deliminated string of acquisition labels to filter by; if not provided, curates over all acquisitions",
            "type": "string",
            "default": ""
        }
    },
    "command": "/bin/run"
}