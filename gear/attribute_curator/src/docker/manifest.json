{
    "name": "attribute-curator",
    "label": "Attribute Curator",
    "description": "Curation of derived data for MQT project",
    "version": "0.0.1a",
    "author": "NACC",
    "maintainer": "NACC <nacchelp@uw.edu>",
    "cite": "",
    "license": "MPL-2.0",
    "url": "https://naccdata.github.io/flywheel-gear-extensions",
    "source": "https://github.com/naccdata/flywheel-gear-extensions",
    "environment": {
        "FLYWHEEL": "/flywheel/v0"
    },
    "custom": {
        "gear-builder": {
            "category": "analysis",
            "image": "naccdata/attribute-curator:0.0.1a"
        },
        "flywheel": {
            "suite": "Curation",
            "show-job": true
        }
    },
    "inputs": {
        "api-key": {
            "base": "api-key",
            "read-only": false
        },
        "derive_rules": {
            "description": "CSV file containing the derivation rules",
            "base": "file",
            "type": {
                "enum": [
                    "tabular data"
                ]
            }
        }
    },
    "config": {
        "dry_run": {
            "description": "Whether to do a dry run",
            "type": "boolean",
            "default": false
        },
        "date_key": {
            "description": "Date key to order data by",
            "type": "string"
        },
        "filename_pattern": {
            "description": "Filename pattern to match on",
            "type": "string",
            "default": "*.json"
        },
        "curation_type": {
            "description": "Curation type - determines curation gear",
            "type": "string",
            "default": "general",
            "enum": [
                "general",
                "uds"
            ]
        }
    },
    "command": "/bin/run"
}