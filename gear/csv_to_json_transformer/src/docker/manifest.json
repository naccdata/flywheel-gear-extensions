{
    "name": "csv-to-json-transformer",
    "label": "CSV to JSON Transformer",
    "description": "Converts rows of CSV file to JSON files",
    "version": "0.0.6",
    "author": "NACC",
    "maintainer": "NACC <nacchelp@uw.edu>",
    "cite": "",
    "license": "MPL-2.0",
    "url": "https://naccdata.github.io/flywheel-gear-extensions",
    "source": "https://github.com/naccdata/flywheel-gear-extensions",
    "environment": {
        "FLYWHEEL": "/flywheel/v0"
    },
    "custom": {
        "gear-builder": {
            "category": "utility",
            "image": "naccdata/csv-to-json-transformer:0.0.6"
        },
        "flywheel": {
            "suite": "Curation",
            "show-job": true
        }
    },
    "inputs": {
        "api-key": {
            "base": "api-key"
        },
        "input_file": {
            "description": "The project-level CSV file with naccid and module",
            "base": "file",
            "type": {
                "enum": [
                    "source code"
                ]
            }
        }
    },
    "config": {
        "required_fields": {
            "description": "List of fields needs to be present in the file",
            "type": "string",
            "default": "naccid, module, visitnum, visitdate"
        }
    },
    "command": "/bin/run"
}