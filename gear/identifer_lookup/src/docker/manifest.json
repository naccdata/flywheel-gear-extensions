{
    "name": "identifer-lookup",
    "label": "NACC ID Lookup",
    "description": "Gear to check for NACC ID for incoming data",
    "version": "0.0.1",
    "author": "NACC",
    "maintainer": "NACC <nacchelp@uw.edu>",
    "cite": "",
    "license": "MPL-2.0",
    "url": "https://naccdata.github.io/flywheel-gear-extensions",
    "source": "https://github.com/naccdata/flywheel-gear-extensions",
    "environment": {
        "FLYWHEEL": "/flywheel/v0"
    },
    "custom": {
        "gear-builder": {
            "category": "utility",
            "image": "naccdata/identifer-lookup:0.0.1"
        },
        "flywheel": {
            "suite": "NACC Data Gears",
            "show-job": true
        }
    },
    "inputs": {
        "api-key": {
            "base": "api-key"
        },
        "input_file": {
            "description": "The CSV input file. Each row must have ADCID and PTID",
            "base": "file",
            "type": {
                "enum": [
                    "tabular data"
                ]
            }
        }
    },
    "config": {
        "dry_run": {
            "description": "Whether to do a dry run",
            "type": "boolean",
            "default": false
        },
        "rds_parameter_path": {
            "description": "SSM parameter path for identifiers RDS credentials",
            "type": "string",
            "default": "/prod/flywheel/gearbot/identifiers"
        },
        "apikey_path_prefix": {
            "description": "The instance specific AWS parameter gearbot path prefix",
            "type": "string",
            "default": "/prod/flywheel/gearbot"

        }
    },
    "command": "/bin/run"
}